<Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute>
<CLSCompliant(False)>
Public Class ScriptMain
    Inherits UserComponent

    Public Overrides Sub Input0_ProcessInputRow(ByVal Row As Input0Buffer)
        If Row.SOCSECURITYNUM_IsNull Then
            Row.Valid = False
        Else
            Row.Valid = ValidateBSN(Row.SOCSECURITYNUM)
        End If
    End Sub

    Private Function ValidateBSN(S As String) As Boolean
        If S = "0" Then
            ValidateBSN = False
        Else
            ValidateBSN = (CheckBSN(S) = vbNullString)
        End If
    End Function

    Private Function Do_11_Proef(S As String) As Boolean
        Dim i As Int16, t As Int32
        t = 0
        For i = 1 To 8
            t += (10 - i) * CInt(Val(S.Substring(i - 1, 1)))
        Next i

        t -= CInt(Val(S.Substring(8, 1)))

        Return (t Mod 11 = 0)
    End Function

    Private Function CheckBSN(ByVal B As String) As String
        Dim i As Int16, R As Boolean
        Select Case B.Length()
            Case 0
                Return "empty"
            Case < 9
                Return "Too short"
            Case 9
                R = True
                For i = 1 To 9
                    Select Case Mid(B, i, 1)
                        Case "0" To "9"
                        Case Else
                            R = False
                    End Select
                Next i

                If R Then
                    Return CType(IIf(Do_11_Proef(B), vbNullString, "violates 11 check"), String)
                Else
                    Return "not numeric"
                End If
            Case Else
                Return "too long"
        End Select
    End Function
End Class
