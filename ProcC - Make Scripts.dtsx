      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;Select Top &quot; + (DT_WSTR, 9) @[User::Extent] + &quot; D.ID, SAF, D.CurrentStep &#xA;From Documents D&#xA;Inner Join FileTypes FT ON D.FileType = FT.ID&#xA;Inner Join [xFdF Templates] AS X ON D.[xFdF Template] = X.ID&#xA;Inner Join [MetadataStatus] AS M ON D.[DocStatus] = M.ID&#xA;Where M.DoConvert = 1&#xA;AND CurrentStep = &quot; + (DT_WSTR, 9) (@[$Package::MainStep]-1)  + &quot;&#xA;AND ProcStatus &lt; 3 &#xA;Order By FT.Sort ASC, X.[Doc Count] DESC, DossierDate ASC&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="DocSQL">
      <DTS:VariableValue
        DTS:DataType="8">Select Top 10000 D.ID, SAF, D.CurrentStep 
From Documents D
Inner Join FileTypes FT ON D.FileType = FT.ID
Inner Join [xFdF Templates] AS X ON D.[xFdF Template] = X.ID
Inner Join [MetadataStatus] AS M ON D.[DocStatus] = M.ID
Where M.DoConvert = 1
AND CurrentStep = 18
AND ProcStatus &lt; 3 
Order By FT.Sort ASC, X.[Doc Count] DESC, DossierDate ASC</DTS:VariableValue>
    </DTS:Variable>
