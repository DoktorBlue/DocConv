<Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute>
<CLSCompliant(False)>
Public Class ScriptMain
    Inherits UserComponent
    Dim VolgNr As Integer = 0
    Public Overrides Sub PreExecute()
        MyBase.PreExecute()
        VolgNr = 0
    End Sub

    Private Sub PrepareLocalDateTime()
        AddCmdLine("for /F ""usebackq tokens=1,2 delims=="" %%i in (`wmic os get LocalDateTime /VALUE 2^>NUL`) do if '.%%i.'=='.LocalDateTime.' set ldt=%%j")
        AddCmdLine("set ldt=%ldt:~0,4%-%ldt:~4,2%-%ldt:~6,2% %ldt:~8,2%:%ldt:~10,2%:%ldt:~12,6%")
    End Sub

    Private Sub PrintHeader()
        AddCmdLine("@echo off")
        AddCmdLine("SetLocal")
        AddCmdLine("")
        Call PrepareLocalDateTime()
        AddCmdLine("echo 0^|%ldt%^|Start^|")
        AddCmdLine("")
        AddCmdLine("")
    End Sub
    Private Sub PrintFooter()
        Call PrepareLocalDateTime()
        AddCmdLine("echo 0^|%ldt%^|End^|")
    End Sub

    Public Overrides Sub Input0_ProcessInputRow(ByVal Row As Input0Buffer)
        Dim InFile As String, OutFile As String
        InFile = SourceSAF(Row.CurrentYear, Row.Archive, Row.Saf)
        OutFile = "%Folder%" & Row.Saf & "." & Row.Ext

        VolgNr += 1
        If VolgNr = 1 Then Call PrintHeader()

        AddCmdLine("set Folder=" & Row.UnzipDestination & "\")
        AddCmdLine("MkDir %Folder% 1>> %~dpn0.siebel 2>> %~dpn0.err")
        Call PrepareLocalDateTime()
        AddCmdLine("echo " & Row.DocID & "^|%ldt%^|Path^|%Folder%")
        AddCmdLine("if not exist %Folder% goto Fail" & Row.DocID)
        AddCmdLine("")

        Call PrepareLocalDateTime()
        AddCmdLine("echo " & Row.DocID & "^|%ldt%^|SAF^|" & InFile & "")
        AddCmdLine("if not exist " & InFile & " goto Fail" & Row.DocID)
        AddCmdLine("")

        AddCmdLine("""" & Row.UnzipProgramm & """ """ & InFile & """ """ & OutFile & """ >> %~dpn0.err 2>> %~dpn0.siebel")
        AddCmdLine("If ERRORLEVEL 0 Set Result=Unzipped")
        AddCmdLine("If ERRORLEVEL 1 Set Result=Failed")
        Call PrepareLocalDateTime()
        AddCmdLine("echo " & Row.DocID & "^|%ldt%^|%Result%^|" & OutFile & "")
        AddCmdLine("goto End" & Row.DocID)
        AddCmdLine("")

        AddCmdLine(":Fail" & Row.DocID)
        Call PrepareLocalDateTime()
        AddCmdLine("echo " & Row.DocID & "^|%ldt%^|Failed^|" & OutFile & "")
        AddCmdLine("")

        AddCmdLine(":End" & Row.DocID)
        AddCmdLine("echo DocConvID^=" & Row.DocID & " >> %~dpn0.err")
        AddCmdLine("")
        AddCmdLine("")

        If VolgNr = Variables.DocCount Then Call PrintFooter()
    End Sub

    Private Function SourceSAF(CurrentYear As Boolean, Archive As String, SAF As String) As String
        Dim SrcPath As String
        If CurrentYear Then
            SrcPath = Left(Archive, Len(Archive) - Len(SAF) - 1)
            If System.IO.File.Exists(SrcPath + "2\" + SAF) Then
                SourceSAF = SrcPath + "2\" + SAF
            Else
                SourceSAF = SrcPath + "\" + SAF
            End If
            '        AddCmdLine("Copy """ & SrcPath + "2\" + SAF & """ ""%Folder%"" 1>> %~dpn0.err 2>> %~dpn0.siebel")
            '        AddCmdLine("If ERRORLEVEL 1 Copy """ & SrcPath + "\" + SAF & """ ""%Folder%"" 1>> %~dpn0.err 2>> %~dpn0.siebel")
        Else
            SourceSAF = Archive
            '        AddCmdLine("Copy """ & Archive & """ ""%Folder%"" 1>> %~dpn0.err 2>> %~dpn0.siebel")
        End If
    End Function

    Public Sub AddCmdLine(CmdLine As String)
        On Error GoTo ErrHandler
        CmdFileBuffer.AddRow()
        CmdFileBuffer.Line = CmdLine
        Exit Sub

ErrHandler:
        ComponentMetaData.FireWarning(1, CmdLine, Err.Description, "", 0)
    End Sub
End Class
