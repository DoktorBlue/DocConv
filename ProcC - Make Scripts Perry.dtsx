Public Class ScriptMain
    Inherits UserComponent
    Dim VolgNr As Integer = 0
    Public Overrides Sub PreExecute()
        MyBase.PreExecute()
        VolgNr = 0
    End Sub

    Private Sub PrepareLocalDateTime()
        AddCmdLine("for /F ""usebackq tokens=1,2 delims=="" %%i in (`wmic os get LocalDateTime /VALUE 2^>NUL`) do if '.%%i.'=='.LocalDateTime.' set ldt=%%j")
        AddCmdLine("set ldt=%ldt:~0,4%-%ldt:~4,2%-%ldt:~6,2% %ldt:~8,2%:%ldt:~10,2%:%ldt:~12,6%")
    End Sub

    Private Sub PrintHeader()
        AddCmdLine("@echo off")
        AddCmdLine("SetLocal")
        AddCmdLine("")
        Call PrepareLocalDateTime()
        AddCmdLine("echo 0^|%ldt%^|Start^|")
        AddCmdLine("")
    End Sub
    Private Sub PrintFooter()
        Call PrepareLocalDateTime()
        AddCmdLine("echo 0^|%ldt%^|End^|")
    End Sub

    Public Overrides Sub Input0_ProcessInputRow(ByVal Row As Input0Buffer)
        Dim OutFile As String
        OutFile = Row.DocPath & "\" & Row.Saf & "." & Row.Ext

        VolgNr = VolgNr + 1
        If VolgNr = 1 Then Call PrintHeader()

        AddCmdLine("MkDir """ & Row.DocPath & """ 1>> %~dpn0.siebel 2>> %~dpn0.err")
        AddCmdLine("Copy """ & Row.Archive & """ """ & OutFile & """ 1>> %~dpn0.siebel 2>> %~dpn0.err")
        AddCmdLine("rem """ & Row.UnzipProgramm & """ """ & Row.DocPath & "\" & Row.Saf & """ """ & OutFile & """ >> %~dpn0.err 2>> %~dpn0.siebel")
        AddCmdLine("If ERRORLEVEL 0 Set Result=Unzipped")
        AddCmdLine("If ERRORLEVEL 1 Set Result=Failed")
        Call PrepareLocalDateTime()
        AddCmdLine("echo " & Row.DocID & "^|%ldt%^|%Result%^|" & OutFile & "")
        AddCmdLine("echo DocConvID^=" & Row.DocID & " >> %~dpn0.err")
        AddCmdLine("")

        If VolgNr = Variables.DocCount Then Call PrintFooter()
    End Sub

    Public Sub AddCmdLine(CmdLine As String)
        On Error GoTo ErrHandler
        CmdFileBuffer.AddRow()
        CmdFileBuffer.Line = CmdLine
        Exit Sub

ErrHandler:
        ComponentMetaData.FireWarning(1, CmdLine, Err.Description, "", 0)
    End Sub
End Class
